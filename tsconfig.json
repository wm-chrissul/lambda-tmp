 {
  "compilerOptions": {
    "target": "es2020",
    "strict": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "module":"commonjs",
    "moduleResolution":"node",
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true, 
    "isolatedModules": true, 
    "outDir": "./built",
    "rootDir": "src"
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
